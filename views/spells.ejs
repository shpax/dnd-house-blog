<div
  class="site-cover site-cover-sm same-height overlay single-page bg-bottom"
  style="background-image: url('/static/images/magic-bg.jpg');"
>
  <div class="container">
    <div class="row same-height justify-content-center">
      <div class="col-md-12 col-lg-10">
        <div class="post-entry text-center">
          <h1 class="">Заклинания</h1>
          <p class="lead mb-4 text-white">Книга игрока, Руководство Занатара</p>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="site-section bg-light">
  <div class="container" id="container">
    <div class="row">
      <div class="col-md-4 col-sm-12">
        <ul class="list-group list-group-horizontal">
          <li class="list-group-item">
            Волшебник
          </li>
          <li class="list-group-item">Друид</li>
          <li class="list-group-item">Колдун</li>
          <li class="list-group-item">Воин</li>
          <li class="list-group-item">Плут</li>
        </ul>
      </div>
      <div class="col-md-8 col-sm-12">
        <table
          class="table table-hover table-borderless shadow-sm bg-white rounded"
        >
          <thead>
            <tr>
              <th scope="col">Ур.</th>
              <th scope="col">Название</th>
            </tr>
          </thead>
          <tbody>
            <% data.forEach(item => { %>

            <tr>
              <th scope="row"><%= item.level %></th>
              <td><%= item.name %></td>
            </tr>
            <% }) %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>
